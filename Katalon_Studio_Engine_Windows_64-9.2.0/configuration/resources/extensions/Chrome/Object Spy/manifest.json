{
	"manifest_version": 3,
	"name": "Katalon Object Spy",
	"description": "Element Spy Chrome extension for Katalon",
	"version": "4.0.0",
	"permissions": [
		"storage"
	],
	"host_permissions": [
		"http://*/",
		"https://*/"
	],
	"icons": {
		"128": "images/object_spy_24.png"
	},
	"content_scripts": [
		{
			"all_frames": true,
			"matches": [
				"http://*/*",
				"https://*/*"
			],
			"match_about_blank": true,
			"js": [
				"jquery-1.11.2.min.js",
				"jquery.color.js",
				"constants.js",
				"chrome_variables_default.js",
				"chrome_variables_init.js",
				"chrome_common.js",
				"common.js",
				"chrome_setup.js",
				"dom_inspector.js",
				"dom_collector.js",
				"process_element.js",
				"main.js"
			]
		}
	],
	"background": {
        "service_worker": "service_worker.js"
	},
	"action": {
		"default_icon": {
			"24": "images/object_spy_24.png"
		},
			"default_title": "Katalon Object Spy",
			"default_popup": "popup.html"
		}
}